{
    "name": "danielsimkus/laravel-slack-emoji-transformer",
    "description": "Transform emojis to images/unicode from a message from Slack Webhook/Events\n\n",
    "license": "MIT",
    "keywords": [
        "laravel",
        "slack"
    ],
    "authors": [
        {
            "name": "Daniel Simkus",
            "email": "dan@happity.io",
            "homepage": "https://happity.io"
        },
        {
            "name": "Ashley Hindle",
            "email": "ashley@happity.io",
            "homepage": "https://happity.io"
        }
    ],
    "autoload": {
        "psr-4": {
          "DanielSimkus\\SlackEmojiTransformer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "DanielSimkus\\SlackEmojiTransformer\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "laravel/framework": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4|^9.3",
        "mockery/mockery": "^1.4"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DanielSimkus\\SlackEmojiTransformer\\Providers\\SlackEmojiTransformerProvider"
            ]
        }
    }
}
